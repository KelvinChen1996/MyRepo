<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:clipToPadding="false"
    android:background="@color/colorwhitegold"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">
    <LinearLayout
        android:id="@+id/franchisedetailprofile_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:background="@color/white"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/building"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:layout_gravity="center"
                        android:id="@+id/franchisedetailprofile_franchise_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:textColor="@color/black"
                        android:text="Franchise Name" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="55dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/franchisedetailprofile_imagecategory"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/automotive_category"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/franchisedetailprofile_category"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textSize="17sp"
                        android:textColor="@color/black"
                        android:text="Category" />
                        <TextView
                            android:id="@+id/franchisedetailprofile_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:textSize="17sp"
                            android:textColor="@color/darkgray"
                            android:text="Outsourcing" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="55dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/since"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/franchisedetailprofile_since"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="2017" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="55dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/investment1"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/franchisedetailprofile_investment"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="Rp. 1.000.000.000" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="55dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/franchisefee1"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/franchisedetailprofile_franchisefee"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="Rp. 1.000.000.000" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="55dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/addresss"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/franchisedetailprofile_location"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="Indonesia" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text=", " />
                    <TextView
                        android:id="@+id/franchisedetailprofile_address"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="Jalan bali" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="55dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/phonenumber1"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/franchisedetailprofile_phonenumber"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="0812-3456-7890" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="55dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/email1"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/franchisedetailprofile_email"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="pizzawillkickyournuts@pizza.net.id" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="55dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/website1"
                        android:layout_marginRight="30dp"/>
                    <TextView
                        android:id="@+id/franchisedetailprofile_website"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:layout_gravity="center"
                        android:textColor="@color/black"
                        android:text="www.pizzawillkickyournuts.com" />
                </LinearLayout>
                <View
                    android:layout_marginLeft="55dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginBottom="5dp"
                    android:background="@android:color/darker_gray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:padding="5dp"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="12sp"
                        android:textColor="@color/darkgray"
                        android:layout_marginBottom="10dp"
                        android:text="Deskripsi" />

                    <TextView
                        android:id="@+id/franchisedetailprofile_deskripsi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:text="asdl;fk kjf;ladsjdl;fkj;asdlkfjaajlf jsadlfjlasdjfl;asdl;fkj;asdlkfjals;dk kjf;ladsjf;lasdjf ;asjdf;lsdajlf jsadlfjlasdjfl;asdl;fkj;asdlkfjals;dk kjf;ladsjf;lasdjf ;asjdf;lsdajlf jsadlfjlasdjfl;asdl;fkj;asdlkfjals;dk kjf;ladsjf;lasdjf ;asjdf;lsdajlf jsadlfjlasdjfl;asdl;fkj;asdlkfjals;dk kjf;ladsjf;lasdjf ;asjdf;lsdajlf jsadlfjlasdjfl;dlkfjals;dk kjf;ladsjf;lasdjf ;asjdf;lsdajlf jsadlfjlasdjfl;" />
                </LinearLayout>
            </LinearLayout>
    </LinearLayout>



</android.support.v4.widget.NestedScrollView>
